---
import Layout from "@/layouts/Layout.astro";
import { ai } from "@/lib/genkit";

const { stream } = await ai.generateStream("Hello, Gemini!");

async function* streamToGenerator() {
  for await (const chunk of stream) {
    yield chunk.text;
  }
}
---

<Layout>
  <pre>
    {streamToGenerator()}
  </pre>
</Layout>
